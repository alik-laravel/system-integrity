{
  "name": "alik-laravel/system-integrity",
  "version": "1.0.0",
  "description": "System integrity and configuration optimization for Laravel applications",
  "type": "library",
  "license": "proprietary",
  "require": {
    "php": "^8.2",
    "illuminate/support": "^11.0|^12.0",
    "illuminate/http": "^11.0|^12.0",
    "illuminate/filesystem": "^11.0|^12.0"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0|^10.0",
    "pestphp/pest": "^3.0",
    "pestphp/pest-plugin-laravel": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Alik\\SystemIntegrity\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Alik\\SystemIntegrity\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Alik\\SystemIntegrity\\Providers\\IntegrityServiceProvider"
      ],
      "aliases": {
        "SystemHealth": "Alik\\SystemIntegrity\\Facades\\SystemHealth"
      }
    }
  },
  "scripts": {
    "test": "pest",
    "test-coverage": "pest --coverage"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "stable"
}
